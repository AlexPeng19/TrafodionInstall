<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>queryConfig</servlet-name>
    <servlet-class>com.esgyn.servlet.QueryConfig</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>queryConfig</servlet-name>
    <url-pattern>/queryConfig</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>queryInstall</servlet-name>
    <servlet-class>com.esgyn.servlet.QueryInstall</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>queryInstall</servlet-name>
    <url-pattern>/queryInstall</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newConfig</servlet-name>
    <servlet-class>com.esgyn.servlet.NewConfig</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newConfig</servlet-name>
    <url-pattern>/newConfig</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>